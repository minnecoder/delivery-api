{
  "name": "delivery-api",
  "version": "1.0.0",
  "description": "API for a number of delivery based applicaitons",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node build/server.js",
    "build": "tsc && tsc-alias",
    "dev": "nodemon build/index.js",
    "dev-ts": "ts-node-dev --respawn --transpile-only server.ts",
    "type-check": "tsc --noEmit",
