    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "path": "^0.12.7",
    "pg": "^8.0.3",
    "sequelize": "^5.21.7",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
